    <Panel hittest="false" class="TalentTreeRoot">

      <Panel hittest="false" class="TalentButtons">
        <Button hittest="true" class="talent_button" onactivate='OnTalentSpell1()'/>

        <Button hittest="true" class="talent_button" onactivate='OnTalentSpell2()'/>

        <Button hittest="true" class="talent_button" onactivate='OnTalentSpell3()'/>

        <Button hittest="true" class="talent_button" onactivate='OnTalentSpell4()'/>

        <Button hittest="true" class="talent_button" onactivate='OnTalentSpell5()'/>

      </Panel>


      <Panel id="TalentContainer" hittest="false">

        <Panel class="TalentSpellContainer" id="TalentPanel1" hittest="false">
          <Label text="TIER 3"/>
          <Panel class="TalentTierBlocked" id="Tier-blocked1"/>
          <Panel class="TalentTier" id="Tier3" hittest="false">
            <Button class="TalentSkillOn">
              <DOTAAbilityImage id="TalentImage" abilityname="" />
            </Panel>
            <Button class="TalentSkillOff">
              <DOTAAbilityImage id="TalentImage" abilityname="" />
            </Panel>
          </Panel>

          <Label text="TIER 2"/>
          <Panel class="TalentTierBlocked" id="Tier-blocked2"/>
          <Panel class="TalentTierAvailable" id="Tier2" hittest="false">
            <Panel class="TalentSkill">
              <DOTAAbilityImage id="TalentImage" abilityname="" />
            </Panel>
            <Panel class="TalentSkill">
              <DOTAAbilityImage id="TalentImage" abilityname="" />
            </Panel>
          
          </Panel>
          
          <Label text="TIER 1"/>
          <Panel class="TalentTierBlocked" id="Tier-blocked3"/>
          <Panel class="TalentTier" id="Tier1" hittest="false">
            <Panel class="TalentSkill">
              <DOTAAbilityImage id="TalentImage" abilityname="" />
            </Panel>
            <Panel class="TalentSkill">
              <DOTAAbilityImage id="TalentImage" abilityname="" />
            </Panel>
          </Panel>
          
          <Panel class="talent-skill-label" hittest="false">
            <Label id="skill-name" text="curseness bond"/>
            <Label id="talent-level" text="RANK UP"/>
          </Panel>

        </Panel>
      </Panel>


        
